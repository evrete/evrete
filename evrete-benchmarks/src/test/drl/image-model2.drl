import org.evrete.benchmarks.models.ml.Image;
import org.evrete.benchmarks.models.ml.ImageProcessor;
import java.util.Arrays;

rule "imagess"
  when
      $img1        : Image()
      $img2        : Image()
      $processor   : ImageProcessor(test($img1.label, $img2.label))
  then
      $processor.compute($img1, $img2);
end

